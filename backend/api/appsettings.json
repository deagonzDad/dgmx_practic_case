{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "H8MWhd/x+PYogNcgFWabjDEXbnFAuyT7ulq3Ox7qQtfT4s4iex5Kspgog2nxYx78StOAFlPTVc4MDlY77vLzPw==",
    "Issuer": "http://localhost:5038/",
    "Audience": "http://localhost:5038/",
    "ExpirationMinutes": 60
  },
  "Encryption": {
    "SecretKey": "YourVerySecretKey",
    "InitVector": "InitializationVec"
  },
  "ConnectionStrings": {
    "SqliteConnection": "Data Source=MyDatabase.db"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Expressions",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Enrich": [
      "FromContext",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/log-prod-.txt",
                  "rollingInterval": "Day",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "@l = 'Warning' or @p.CDebug = true"
                }
              }
            ]
          }
        }
      }
    ],
    "Properties": {
      "ApplicationName": "DGMX Hotel Dev"
    }
  }
}